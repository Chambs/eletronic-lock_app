{
  "name": "user-services",
  "version": "1.0.0",
  "main": "controllers.js",
  "scripts": {
    "test": "jest",
    "test:e2e": "jest tests/e2e/*.spec.js",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "start": "node server.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.9.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "multer": "^2.0.0",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": ["<rootDir>/tests/setup.js"],
    "testMatch": [
      "**/tests/**/*.spec.js",
      "**/tests/e2e/**/*.spec.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "collectCoverageFrom": [
      "**/*.js",
      "!**/node_modules/**",
      "!**/tests/**",
      "!server.js"
    ],
    "testTimeout": 30000
  },
  "description": ""
}
